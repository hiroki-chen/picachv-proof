{
  "files.exclude": {
    "**/*.vo": true,
    "**/*.vok": true,
    "**/*.vos": true,
    "**/*.aux": true,
    "**/*.glob": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.classpath": true,
    "**/.factorypath": true
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "storage",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "constant.language",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "support.class.builtin",
        "settings": {
          "fontStyle": "bold"
        }
      }
    ]
  }
}